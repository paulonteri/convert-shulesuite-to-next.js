{
    "hosting": {
        "site": "shulesuite",
        "public": "out",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**",
            "**/.next/**"
        ],
        // Drops `.html` from uploaded URLs
        "cleanUrls": true,
        // Removes trailing slashes from URLs
        "trailingSlash": false
    }
}
